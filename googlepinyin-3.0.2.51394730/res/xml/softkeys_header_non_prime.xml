<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_template id="@+id/softkey_template_non_prime_tab_active" layout="@layout/softkey_label_small_bold_center_icon_bottom">
            <action type="PRESS" keycode="UNKNOWN" />
            <label location="@id/label" value="$param_text$" />
            <icon location="@id/icon_bottom" value="@drawable/ic_bar_green" />
        </softkey_template>
        <softkey_list template_id="@id/softkey_template_non_prime_tab_active">
            <softkey id="@id/softkey_non_prime_tab_digit_active" param_text="123" />
            <softkey id="@id/softkey_non_prime_tab_symbol_active" param_text="\@?#" />
            <softkey id="@id/softkey_non_prime_tab_web_active" param_text="Web" />
        </softkey_list>
        <softkey id="@id/softkey_non_prime_tab_smiley_active" layout="@layout/softkey_icon_animation_center_icon_bottom">
            <action type="PRESS" keycode="UNKNOWN" />
            <icon location="@id/icon" value="@drawable/anim_smiley" />
            <icon location="@id/icon_bottom" value="@drawable/ic_bar_green" />
        </softkey>
        <softkey_template id="@+id/softkey_template_non_prime_tab_deactive" layout="@layout/softkey_label_small_bold">
            <action type="PRESS" keycode="SWITCH_KEYBOARD" data="$param_data$" />
            <label location="@id/label" value="$param_text$" />
        </softkey_template>
        <softkey_list template_id="@id/softkey_template_non_prime_tab_deactive">
            <softkey id="@id/softkey_non_prime_tab_digit_deactive" param_text="123" param_data="digit" />
            <softkey id="@id/softkey_non_prime_tab_symbol_deactive" param_text="\@?#" param_data="symbol" />
            <softkey id="@id/softkey_non_prime_tab_web_deactive" param_text="Web" param_data="web" />
        </softkey_list>
        <softkey id="@id/softkey_non_prime_tab_smiley_deactive" layout="@layout/softkey_icon">
            <action type="PRESS" keycode="SWITCH_KEYBOARD" data="smiley" />
            <icon location="@id/icon" value="@drawable/ic_smiley" />
        </softkey>
    </softkeys>
</framework>