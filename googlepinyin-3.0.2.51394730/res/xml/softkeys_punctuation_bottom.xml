<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey id="@id/softkey_bottom_comma" layout="@layout/softkey_label_medium_bottom" popup_timing="ON_TOUCH" multi_touch="true">
            <action type="PRESS" keycode="COMMA" data="," intention="COMMIT" />
            <label location="@id/label_bottom" value="," />
        </softkey>
        <softkey id="@id/softkey_bottom_sentence" layout="@layout/softkey_label_medium_bottom" popup_timing="ON_TOUCH" multi_touch="true">
            <action type="PRESS" keycode="PERIOD" data="." intention="COMMIT" />
            <label location="@id/label_bottom" value="." />
        </softkey>
        <softkey id="@id/softkey_bottom_at" layout="@layout/softkey_label_medium" popup_timing="ON_TOUCH" multi_touch="true">
            <action type="PRESS" keycode="AT" data="\@" intention="COMMIT" />
            <label location="@id/label" value="\@" />
        </softkey>
        <softkey id="@id/softkey_bottom_slash" layout="@layout/softkey_label_medium" popup_timing="ON_TOUCH" multi_touch="true">
            <action type="PRESS" keycode="SLASH" data="/" intention="COMMIT" />
            <label location="@id/label" value="/" />
        </softkey>
        <softkey id="@id/softkey_bottom_com" layout="@layout/softkey_label_tiny" popup_timing="ON_TOUCH" multi_touch="true">
            <action type="PRESS" keycode="PLAIN_TEXT" data=".com" intention="COMMIT" />
            <label location="@id/label" value=".com" />
        </softkey>
        <softkey_list splitter=",">
            <softkey id="@id/softkey_bottom_com" layout="@layout/softkey_label_tiny_center_label_sub" popup_timing="ON_TOUCH" multi_touch="true">
                <action type="PRESS" keycode="PLAIN_TEXT" data=".com" intention="COMMIT" />
                <action type="LONG_PRESS" data=".cn,.net,.org,.edu,.gov" keycode="PLAIN_TEXT" intention="COMMIT" popup_label=".cn,.net,.org,.edu,.gov" popup_layout="@layout/popup_bubble_rectangular_label_small" />
                <label location="@id/label" value=".com" />
                <label location="@id/label_sub" value="…" />
            </softkey>
            <softkey id="@id/softkey_bottom_sentence_popup_smiley" layout="@layout/softkey_label_medium_bottom_label_sub" popup_timing="ON_TOUCH" multi_touch="true">
                <action type="PRESS" keycode="PERIOD" data="." intention="COMMIT" />
                <action type="LONG_PRESS" data=":P,:D,:(,:),^_^" keycode="PLAIN_TEXT" intention="COMMIT" popup_label=":P,:D,:(,:),^_^" popup_layout="@layout/popup_bubble_rectangular_label_small" />
                <label location="@id/label_bottom" value="." />
                <label location="@id/label_sub" value="…" />
            </softkey>
            <softkey id="@id/softkey_bottom_sentence_popup_punctuation" layout="@layout/softkey_label_medium_bottom_label_sub" popup_timing="ON_TOUCH" multi_touch="true">
                <action type="PRESS" keycode="PERIOD" data="." intention="COMMIT" />
                <action type="LONG_PRESS" data="!,?,…,:,~,:P,:D,:(,:),^_^" keycode="PLAIN_TEXT" intention="COMMIT" popup_label="!,?,…,:,~,:P,:D,:(,:),^_^" popup_layout="@layout/popup_bubble_rectangular_label_small" />
                <label location="@id/label_bottom" value="." />
                <label location="@id/label_sub" value="…" />
            </softkey>
        </softkey_list>
    </softkeys>
</framework>