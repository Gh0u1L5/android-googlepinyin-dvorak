<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_template id="@+id/softkey_template_punctuation_qwerty_tablet_10" layout="@layout/softkey_label_medium_bold" slide_sensitivity="NORMAL" multi_touch="true">
            <action type="PRESS" data="$press_data$" keycode="PLAIN_TEXT" intention="COMMIT" />
            <label location="@id/label" value="$press_data$" />
        </softkey_template>
        <softkey_list template_id="@id/softkey_template_punctuation_qwerty_tablet_10">
            <softkey id="@id/softkey_question" press_data="\?" />
            <softkey id="@id/softkey_exclamation" press_data="!" />
            <softkey id="@id/softkey_colon" press_data=":" />
        </softkey_list>
    </softkeys>
</framework>