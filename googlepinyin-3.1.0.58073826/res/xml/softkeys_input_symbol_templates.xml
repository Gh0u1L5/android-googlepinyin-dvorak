<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_template id="@id/softkey_template_symbol_number" layout="@layout/softkey_label_medium_bold" multi_touch="false">
            <action type="PRESS" keycode="$param_keycode$" data="$param_data$" intention="COMMIT" />
            <label location="@id/label" value="$param_data$" />
        </softkey_template>
        <softkey_template id="@id/softkey_template_light_symbol" layout="@layout/softkey_label_medium" multi_touch="false" content_description="$desc$">
            <action type="PRESS" keycode="SHORT_TEXT" data="$param_data$" intention="COMMIT" />
            <label location="@id/label" value="$param_data$" />
        </softkey_template>
    </softkeys>
</framework>